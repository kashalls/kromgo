metrics:
  - name: node_cpu_usage
    query: "round(cluster:node_cpu:ratio_rate5m * 100, 0.1)"
    prefix: ""      # Optional
    suffix: "%"     # Optional
    colors:         # Optional
      - color: "green"
        min: 0
        max: 35
      - color: "orange"
        min: 36
        max: 75
      - color: "red"
        min: 76
        max: 1000